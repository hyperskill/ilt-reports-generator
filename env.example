# Supabase Configuration
# Get these values from your Supabase project settings: https://app.supabase.com/project/_/settings/api

NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Service role key for admin operations (keep this secret!)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# LiteLLM Configuration
# Internal Hyperskill LiteLLM proxy
LITELLM_API_KEY=your_litellm_api_key
LITELLM_BASE_URL=your_litellm_base_url

# Cogniterra API Configuration
# For fetching course structure (module and lesson names)
COGNITERRA_API_URL=https://cogniterra.org
COGNITERRA_CLIENT_ID=your_cogniterra_client_id
COGNITERRA_CLIENT_SECRET=your_cogniterra_client_secret

# Testing Credentials (for automated LLM tests)
# Add your admin account credentials to enable automatic authentication in test scripts
TEST_ADMIN_EMAIL=your-admin@email.com
TEST_ADMIN_PASSWORD=your-password

